<Window x:Class="aawd.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:aawd"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>

        </Grid.RowDefinitions>
        <TextBox Height="40" Width="300"  Grid.Row="0" x:Name="FIO"/>
        <TextBox Height="40" Width="300"  Grid.Row="1" x:Name="Login"/>
        <TextBox Height="40" Width="300"  Grid.Row="2" x:Name="Password"/>
        <Button Height="40" Width="300" Grid.Row="3"  Content="Регистрация" Click="Button_Click_1"></Button>
        <Button Height="40" Width="300" Grid.Row="4"  Content="Авторизация" Click="Button_Click"></Button>
    </Grid>
</Window>
